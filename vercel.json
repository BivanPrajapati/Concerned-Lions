{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/generate-tree",
            "dest": "api/main.py",
            "methods": [
                "POST",
                "GET"
            ]
        },
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*\\.(js|css|html|png|jpg|jpeg|gif|svg|ico|json))",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            },
            "dest": "FrontEnd/$1"
        },
        {
            "src": "/",
            "dest": "FrontEnd/ConnectedLions.html"
        }
    ],
    "functions": {
        "api/*.py": {
            "memory": 3008,
            "maxDuration": 60
        }
    }
}